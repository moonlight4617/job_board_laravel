{
    "containerDefinitions": [
        {
            "name": "nginx",
            "image": "502674413540.dkr.ecr.ap-northeast-1.amazonaws.com/jobpair-prod-nginx:latest",
            "cpu": 0,
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp"
                }
            ],
            "essential": true,
            "environment": [],
            "mountPoints": [],
            "volumesFrom": [],
            "secrets": [],
            "dependsOn": [
                {
                    "containerName": "php",
                    "condition": "START"
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/jobpair-prod/nginx",
                    "awslogs-region": "ap-northeast-1",
                    "awslogs-stream-prefix": "ecs"
                }
            },
            "systemControls": []
        },
        {
            "name": "php",
            "image": "502674413540.dkr.ecr.ap-northeast-1.amazonaws.com/jobpair-prod-php:latest",
            "cpu": 0,
            "portMappings": [],
            "essential": true,
            "environment": [],
            "mountPoints": [],
            "volumesFrom": [],
            "secrets": [
                {
                    "name": "APP_KEY",
                    "valueFrom": "/jobpair/prod/APP__KEY"
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/jobpair-prod/php",
                    "awslogs-region": "ap-northeast-1",
                    "awslogs-stream-prefix": "ecs"
                }
            },
            "systemControls": []
        }
    ],
    "family": "jobpair-prod",
    "taskRoleArn": "arn:aws:iam::502674413540:role/jobpair-prod-ecs-task",
    "executionRoleArn": "arn:aws:iam::502674413540:role/jobpair-prod-ecs-task-execution",
    "networkMode": "awsvpc",
    "volumes": [],
    "placementConstraints": [],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "256",
    "memory": "512"
}
